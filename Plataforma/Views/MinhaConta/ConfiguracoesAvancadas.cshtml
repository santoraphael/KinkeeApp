
@{
    ViewBag.Title = "Assinatura";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/modules/css/assinatura.css" rel="stylesheet" />

<style>
    .sl-modal.sl-subscription-modal.injected .plans-list .plan h1, .sl-modal.sl-subscription-modal.injected .small-discount .plans-list .plan h1{
        font-size: 37px;
    }
    
</style>
<div class="myaccount">

    <div class="container-fluid myaccount-header">

        <div class="container-flex">

            <div class="row">

                <div class="col-md-4 hidden-xs hidden-sm">

                    <h2>Minha conta</h2>

                </div>

                <div class="col-md-8">

                    <div class="myaccount-options dropdown">

                        <button class="sl-btn btn-link myaccount-button-options js-click-menu">
                            Configurações<i></i>
                        </button>

                        <ul class="myaccount-nav dropdown-menu js-mostra-esconde-menu">

                            <li class="active">
                                <a href="#">
                                    Configurações<i></i>
                                </a>
                            </li>

                            <li class="">
                                <a href="/MinhaConta/Assinatura/">
                                    Assinatura<i></i>
                                </a>
                            </li>

                            <li class="">
                                <a href="/MinhaConta/Privacidade/">
                                    Segurança e privacidade<i></i>
                                </a>
                            </li>

                        </ul>

                    </div>

                </div>

            </div>

        </div>

    </div>

    <div class="container-flex">
        <div class="row ng-scope">
            <div class="col-sm-12 col-md-8">

                <div class="row">
                    <div class="col-md-12">
                        <div class="panel myaccount-panel">
                            <div class="panel-heading">
                                <h3>Informações da conta</h3>
                            </div>
                            <div class="panel-body account-info">
                                
                                <div role="form" class="myaccount-edit myaccount-username">
                                    <div class="form-group">
                                        <div class="myaccount-edit-header js-edit-username-header">
                                            <h6>Kinkee.me/</h6>
                                            <div class="myaccount-label">
                                                <h4>
                                                    @ViewBag.Usuario.Usuario
                                                </h4>
                                                
                                                @*<button type="button" class="sl-btn btn-link btn-open-edit js-btn-open-edit-username">
                                                    <i></i>
                                                    Alterar
                                                </button>*@
                                            </div>
                                        </div>
                                        <div class="myaccount-edit-body js-edit-username">
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <div class="alert alert-info js-alert-username" role="alert">
                                                        O nome de usuário deve ser sem espaços e caracteres especiais como (&%$!). Exemplo de Nome de usuário <strong>Válido:</strong> RAPHAELSANTO, BABYLINDA123.
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table">
                                                            <thead>
                                                                <tr>
                                                                    <th><h6>Nome de Usuário</h6></th>
                                                                    <th><h6></h6></th>
                                                                    <th></th>
                                                                </tr>
                                                            </thead>
                                                            
                                                        </table>
                                                    </div>
                                                    <div class="form-email ng-hide" ng-show="account.email.new">
                                                        <form name="form" class="css-form ng-pristine ng-valid ng-valid-email ng-valid-maxlength" novalidate="">
                                                            <div class="row">
                                                                <div class="col-xs-12 col-sm-6 col-md-6">
                                                                    <input type="email" id="inputEnterUsername" name="uEmail" style="text-transform: uppercase;" maxlength="100" class="form-control input-enter-email ng-pristine ng-untouched ng-valid ng-empty ng-valid-email ng-valid-maxlength" tabindex="1" placeholder="Digite o Nome de Usuário...">
                                                                </div>
                                                                <div class="hidden-xs col-sm-1 col-md-1 js-load-username" style="display:none;">
                                                                    <span class="sl-svg-inline loading sl-svg-inline ng-hide" ng-show="account.email.savingEmail" svg-icon="loading-inline" width="36" height="36" data-fill="#CC6FC5">
                                                                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 45 45" width="36" height="36" fill="#CC6FC5">
                                                                            <circle transform="translate(8 0)" cx="0" cy="16" r="0.0515466">
                                                                                <animate attributeName="r" values="0; 4; 0; 0" dur="1.2s" repeatCount="indefinite" begin="0" keyTimes="0;0.2;0.7;1" keySplines="0.2 0.2 0.4 0.8;0.2 0.6 0.4 0.8;0.2 0.6 0.4 0.8" calcMode="spline"></animate>
                                                                            </circle>
                                                                            <circle transform="translate(16 0)" cx="0" cy="16" r="1.0161">
                                                                                <animate attributeName="r" values="0; 4; 0; 0" dur="1.2s" repeatCount="indefinite" begin="0.3" keyTimes="0;0.2;0.7;1" keySplines="0.2 0.2 0.4 0.8;0.2 0.6 0.4 0.8;0.2 0.6 0.4 0.8" calcMode="spline"></animate>
                                                                            </circle>
                                                                            <circle transform="translate(24 0)" cx="0" cy="16" r="3.85623">
                                                                                <animate attributeName="r" values="0; 4; 0; 0" dur="1.2s" repeatCount="indefinite" begin="0.6" keyTimes="0;0.2;0.7;1" keySplines="0.2 0.2 0.4 0.8;0.2 0.6 0.4 0.8;0.2 0.6 0.4 0.8" calcMode="spline"></animate>
                                                                            </circle>
                                                                        </svg>
                                                                    </span>
                                                                </div>
                                                                <div class="col-xs-12 col-sm-5 col-md-5 text-right">
                                                                    <button class="sl-btn btn-default js-btn-cancel-edit-username" type="button" tabindex="3">
                                                                        Cancelar
                                                                    </button>
                                                                    @if (ViewBag.Usuario.ContaGold)
                                                                    {
                                                                        <button class="sl-btn btn-primary js-btn-salvar-Username-Gold" onclick="SalvarUsuario()"   disabled="disabled" type="button" tabindex="2">
                                                                            Salvar
                                                                        </button>
                                                                    }
                                                                    else
                                                                    {
                                                                        <button class="sl-btn btn-primary js-btn-salvar-Username" onclick="ChamarPainelAssinatura()" type="button" tabindex="2">
                                                                            Salvar
                                                                        </button>
                                                                    }
                                                                    
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                            <br>
                                        </div>
                                    </div>
                                </div>


