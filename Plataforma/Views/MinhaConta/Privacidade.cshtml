    function resetJsDivulgacao() {
        $('.js-divulgacao-ativada').css('display', 'none');
        $('.js-divulgacao-desativada').css('display', 'none');
    }

    function AddRemoveDivulgacaoPerfil(permissao) {
        $.ajax({
            url: "/MinhaConta/AddRemoveDivulgacaoPerfil",
            data: {
                'permissao': permissao
            },

            type: 'POST',
            complete: function (jqXHR) {
                if (jqXHR.readyState === 4) {

                    //resetJsDivulgacao();
                    if (permissao) {
                        $('.js-divulgacao-ativada').css('display', 'block');
                    }
                    else {

                        $('.js-divulgacao-desativada').css('display', 'block');
                    }

                }
            },
            success: function (jqXHR) {
                //$(list).parent('.js-row-user').remove();
            },
            error: function () {
                //Notification();
            }
        });
    }

    $('.js-open-blockedPanel').on("click", function (e) {
        //alert('eet');
        $('#usuarios-bloqueados-panel').css('display', 'block');
    });



