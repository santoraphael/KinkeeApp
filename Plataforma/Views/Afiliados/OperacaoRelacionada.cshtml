        <p>@ViewBag.Operacao.Id</p>
        <p>@ViewBag.Operacao.userId</p>
        <p>@ViewBag.Operacao.subscriptionId</p>
        <br />
        <hr />
        <h3>PLANO</h3>
        <p>@ViewBag.Operacao.plan.id</p>
        <p>@ViewBag.Operacao.plan.amount</p>
        <p>@ViewBag.Operacao.plan.days</p>
        <p>@ViewBag.Operacao.plan.name</p>
