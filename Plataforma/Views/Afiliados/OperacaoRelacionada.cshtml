        <p>@ViewBag.Operacao.plan.date_created</p>
        <p>@ViewBag.Operacao.plan.payment_methods</p>
        <p>@ViewBag.Operacao.plan.color</p>
        <p>@ViewBag.Operacao.plan.charges</p>
        <p>@ViewBag.Operacao.plan.installments</p>
        <p>@ViewBag.Operacao.plan.invoice_reminder</p>
        <p>@ViewBag.Operacao.plan.payment_deadline_charges_interval</p>

        <br />
        <hr />
        <h3>TRANSACAO</h3>
        <p>@ViewBag.Operacao.current_transaction.status</p>
        <p>@ViewBag.Operacao.current_transaction.refuse_reason</p>
