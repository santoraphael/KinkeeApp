
@{
    Layout = null;
}

@using (Html.BeginForm("EnviarConvite", "EnviarConvite", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "sl-form ng-pristine ng-invalid ng-invalid-required", @name = "signinForm", @novalidate = "" }))
{
    <input type="text" name="Email" id="Email" />
    <input type="submit" value="Enviar" />
}

@if(ViewBag.Quantidade != null)
{
    <p>@ViewBag.Quantidade</p>
}