    <script>
        $(document).ready(function () {
            FotoPerfil();
            PerfilVerificadoAtivo();
            function FotoPerfil() {
                $.get('@Url.Action("FotoPerfil", "Dating")', function (data) {
                    $('.js-img-perfil-circle').attr("src", data);
                    $('.js-img-destaque').attr("src", data);
                    //$('.top-image ').attr("src", data);
                });
                //return data;
            }
            function PerfilSugeridoAtivo() {
                $.get('@Url.Action("PerfilSugeridoAtivo", "Dating")', function (data) {
                    if ("True" === data)
                    {
                        $('.sl-highlight').css("display", "none");
                        $('.js-label-sugerido').html('Perfil Destacado');
                        $('.js-label-sugerido-complemento').css("display", "none");
                    }
                });
                //return data;
            }
            function PerfilVerificadoAtivo() {
                $.get('@Url.Action("PerfilVerificadoAtivo", "Dating")', function (data) {
                    if ("True" === data)
                    {
                        $('.sl-highlight').css("display", "none");
                        $('.js-label-sugerido').html('PERFIL VERIFICADO');
                        $('.js-label-sugerido-complemento').css("display", "none");
                        FotoPerfil();
                    }
                });
                //return data;
            }
        });
    </script>
