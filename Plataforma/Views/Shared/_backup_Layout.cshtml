                    'dialog.main.button.unsubscribe': 'DESABILITAR',
                    'dialog.blocked.title': 'Desbloquear notificações',
                    'dialog.blocked.message': "Siga as instrições para habilitar:"
                }
            });
            OneSignal.getUserId(function (userId) {
                //console.log("OneSignal User ID:", userId);
                // (Output) OneSignal User ID: 270a35cd-4dda-4b3f-b04e-41d7463a2316
                SalvarUserID(userId);
            });
        });
        $(document).ready(function () {
            //SalvarUserID('d10c8ab3-8f5f-4c93-aa19-9f65e46abb85');
        });
        function SalvarUserID(userId) {
            $.ajax({
                url: "Notification/SalvarUserID",
                data: {
                    'userId': userId
                },
                dataType: "json",
                type: 'POST',
                success: function (data) {
                    $('.loading-header').text(data);
                },
                error: function () {
                }
            });
        }
    </script>
