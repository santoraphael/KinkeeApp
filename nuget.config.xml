<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
		<add key="MyPrivateFeed" value="https://myprivatefeed.com/nuget" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="MyPrivateFeed">
			<package pattern="MinhaEmpresa.*" />
		</packageSource>
	</packageSourceMapping>
</configuration>

